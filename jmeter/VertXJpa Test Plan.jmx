            <stringProp name="minimumValue">15</stringProp>
            <stringProp name="outputFormat">00</stringProp>
            <boolProp name="perThread">false</boolProp>
            <stringProp name="randomSeed"></stringProp>
            <stringProp name="variableName">USER_AGE</stringProp>
          </RandomVariableConfig>
          <hashTree/>
          <CSVDataSet guiclass="TestBeanGUI" testclass="CSVDataSet" testname="CSV: First Names" enabled="true">
